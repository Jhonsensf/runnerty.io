{
  "name": "universalapp",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tsc:server": "tsc -p server/tsconfig.ssr.json",
    "build:client-and-server-bundles": "ng build --prod --aot && ng build --prod --aot --app 1 --output-hashing=false",
    "build:u": "ng build --prod && ng run ssrtest:server",
    "build:universal": "npm run build:client-and-server-bundles && npm run tsc:server",
    "serve:u": "DISABLE_FIREBASE=true node dist/index.js",
    "serve:universal": "DISABLE_FIREBASE=true node dist/index.js",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:firebase": "firebase serve --only functions,hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/platform-server": "^6.0.2",
    "@angular/router": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "angular2-cookie": "^1.2.6",
    "cookieconsent": "^3.0.6",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "firebase-admin": "^5.12.1",
    "firebase-functions": "^1.0.3",
    "ngx-cookieconsent": "^1.0.1",
    "ngx-page-scroll": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.2.1",
    "ts-loader": "^4.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "reflect-metadata": "^0.1.12",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^2.1.3"
  }
}
